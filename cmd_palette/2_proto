    echo "for example, this"
}

keep_it () {
    echo "i like this command, can i keep it?"
}

smell_instinct () {
    echo -e "it\'s like another sensory system"
}

sibilant () {
    echo "eh this lang is too hissy"
}
go_crazy () {
    echo -e "${SYS_YELLOW}mag${SYS_CYAN}ic exi${SYS_WHITE}st${SYS_MAGENTA}s"
}
instinct () {
    echo "just lock in"
}
meddle_this () {
    echo -e "\n\n\n${SYS_YELLOW}xhqrbklbnm${SYS_CYAN}ooOoOOiii"
}
oh_my_shidd () {
    c=".secret_example_dir"
    d1=badabingafile
    echo "kaboom"
    mkdir $c
    touch $c/$d1
    mv $c/$d1 ~/$d1
    printf "%s\n%s\n%s" "life has many doors" "yes" "can\'t you see\?" > ~/$d1
}

reciprocal_binary () {
    buff0="7_qbuffer"
    for i in {1..1023}; do
        echo $(perl -e "printf \"%2x  %0b\n\", $i , (2**53)/$i;")
    done | sed -e 's/0/./g' -e 's/1/o/g' > $buff0
}

