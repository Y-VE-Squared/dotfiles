hey_a_fn () { echo -e "check this out, whvvuvuvuvuvvsh"; echo -e "whvvuvuvuvvvvvsh"; echo "ok anyway here's a thing"; cat ~/sandbox.local/lab_lang/longwords.txt | awk 'NR % 2 == 0'
emacsc cat ~/sandbox.local/1_refresh.sh; echo "hehehhehehe ok all done"; }


feel () {
  $1 && echo "yea" || echo "nah"
}

get_ntsc () {
  echo $(perl -e "printf \"%f\",60*(111*36*36*36)/(60*60*60*24)")
}

extract_audio () {
  x1="input-video.avi"
  x2="output-audio.aac"
  ffmpeg -i $x1 -vn -acodec copy $x2
}

take_sound_sample_from_vid () {
  x1="sample.avi"
  x2="sample.mp3"
  t0=00:03:05
  t=00:00:45.0
  ffmpeg -i $x1 -ss $t0 -t $t -q:a 0 -map a $x2
}
clip_vid__wip () {
  x1=$1
  x2=$2
  t0=$3
  t=$4
  ffmpeg -i $x1 -ss $t0 -t $t -c copy $x2
}

checksum_example () {
  echo "how to do checksum with sha (example w\/ 256-bit):"
  echo "shasum -a 256 file.thingy\n\n"
  echo "you can do it like this too (note the star placement):"
  echo "echo \"your_checksum_here *file.thingy \| shasum -a 256 --check\" shasum -a 256 file.thingy\n\n"
}

ytdl-output-template-example () {
  echo "yt-dlp: how to use the -o option"
  echo "example:"
  echo "yt-dlp -o %(STRING)s video.com/link123"
}
